<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bw.zyj.otheracticitylaunchtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:launchMode="standard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InstanceActivity"
            android:launchMode="singleInstance" />
        <activity
            android:name=".TopActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".TaskActivity"
            android:launchMode="singleTask" />
        <activity android:name=".TopTestActivity"
            android:launchMode="singleTop"/>
        <activity android:name=".StandardTestActivity"
            android:launchMode="standard"/>
        <activity android:name=".TaskTestActivity"
            android:launchMode="singleTask"/>
        <activity android:name=".InstanceTestActivity"
            android:launchMode="singleInstance"></activity>
    </application>

</manifest>